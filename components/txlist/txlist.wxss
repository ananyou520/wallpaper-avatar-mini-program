.bzWrapper {
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: center;
    justify-content: space-evenly;
    margin-top: 35px;
}

.bzWrapper,.item {
    background-color: #f6f6f6;
}

.item {
    display: inline-block;
    width: 33%;
    text-align: center;
    margin-bottom: 2px;
    position: relative;
}

.item image {
    width: 100%;
    height: 100%;
}
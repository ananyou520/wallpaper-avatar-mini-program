
.tag {
    border: 1rpx solid #111b22;
    color: #111b22;
}

.tag,.tags {
    border-radius: 8rpx;
    font-size: 12px;
    line-height: 46rpx;
    padding: 0 20rpx;
}

.tags {
    background-image: linear-gradient(100deg,rgba(0,0,0,.3),rgba(0,0,0,.94));
    color: #fff;
}

.tab {
    margin-bottom: 15rpx;
    margin-right: 15rpx;
    position: relative;
}

.tab image {
    background-color: #f5f5f5;
}

.btns {
    width: 530rpx;
}

.btns .title {
    color: #000;
    font-weight: 550;
}

.border-bottom {
    border-bottom: 1rpx solid #f3f4f6;
    position: relative;
}

.add {
    background-color: #fff;
    border-radius: 120rpx;
    bottom: 300rpx;
    box-shadow: 0 1px 8px #c8c8c8;
    left: 30rpx;
    position: fixed;
}

.add,.add image {
    height: 80rpx;
    width: 80rpx;
}

@-webkit-keyframes tiaobig {
    0% {
        transform: scale(.92);
    }

    25% {
        transform: scale(1.08);
    }

    50% {
        transform: scale(.96);
    }

    75% {
        transform: scale(1.06);
    }

    100% {
        transform: scale(.94);
    }
}

@keyframes tiaobig {
    0% {
        transform: scale(.92);
    }

    25% {
        transform: scale(1.08);
    }

    50% {
        transform: scale(.96);
    }

    75% {
        transform: scale(1.06);
    }

    100% {
        transform: scale(.94);
    }
}

.cover {
    background-color: #fff;
    border-radius: 12rpx;
}